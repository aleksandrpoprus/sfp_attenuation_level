<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="vBox" stylesheets="@res/html/style/main_style.css"
      xmlns="http://javafx.com/javafx/10.0.2-internal" fx:controller="main.Controller">

    <CheckBox fx:id="check_box" styleClass="check-box" text="оставить только последний результат">
        <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </VBox.margin>
    </CheckBox>

    <Button fx:id="buttonCSV" styleClass="button" text="открыть файл *.csv">
        <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </VBox.margin>
    </Button>
    <WebView fx:id="webView" VBox.vgrow="ALWAYS"/>
</VBox>
